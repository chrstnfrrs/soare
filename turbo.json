{
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": ["dist/**", ".next/**"]
    },
    "coverage": {
      "dependsOn": ["^coverage"],
      "outputs": ["coverage/**"]
    },
    "dev": {
      "dependsOn": ["soare-db#docker:postgres"],
      "cache": false
    },
    "docker:postgres": {
      "cache": false
    },
    "lint": {
      "outputs": []
    },
    "test": {
      "outputs": []
    }
  }
}
